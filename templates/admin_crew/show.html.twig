{% extends 'baseAdmin.html.twig' %}

{% block title %}Crew
{% endblock %}

{% block stylesheets %}
	{{ encore_entry_link_tags('add_crew') }}
{% endblock %}


{% block body %}
	<h1>Crew</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ crew.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ crew.Name }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ crew.Description }}</td>
			</tr>
			<tr>
				<th>Number</th>
				<td>{{ crew.Number }}</td>
			</tr>
			<tr>
				<th>Image</th>
				<td>{{ crew.Image }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_admin_crew_index') }}">back to list</a>

	<a href="{{ path('app_admin_crew_edit', {'id': crew.id}) }}">edit</a>

	{{ include('admin_crew/_delete_form.html.twig') }}
{% endblock %}
