{% extends 'baseAdmin.html.twig' %}

{% block title %}Member
{% endblock %}

{% block body %}
	<h1>Member</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ member.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ member.name }}</td>
			</tr>
			<tr>
				<th>Photo</th>
				<td>{{ member.photo }}</td>
			</tr>
			<tr>
				<th>Age</th>
				<td>{{ member.age }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_admin_member_index') }}">back to list</a>

	<a href="{{ path('app_admin_member_edit', {'id': member.id}) }}">edit</a>

	{{ include('admin_member/_delete_form.html.twig') }}
